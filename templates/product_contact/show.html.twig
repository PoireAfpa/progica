{% extends 'base.html.twig' %}

{% block title %}ProductContact{% endblock %}

{% block body %}
    <h1>ProductContact</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ product_contact.id }}</td>
            </tr>
            <tr>
                <th>LastName</th>
                <td>{{ product_contact.lastName }}</td>
            </tr>
            <tr>
                <th>FirstName</th>
                <td>{{ product_contact.firstName }}</td>
            </tr>
            <tr>
                <th>Tel</th>
                <td>{{ product_contact.tel }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ product_contact.email }}</td>
            </tr>
            <tr>
                <th>Comment</th>
                <td>{{ product_contact.comment }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_product_contact_index') }}">back to list</a>

    <a href="{{ path('app_product_contact_edit', {'id': product_contact.id}) }}">edit</a>

    {{ include('product_contact/_delete_form.html.twig') }}
